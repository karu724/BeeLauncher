<div id="settingsContainer" style="display: none;">
    <div id="settingsContainerLeft">
        <div id="settingsNavContainer">
            <div id="settingsNavHeader">
                <span id="settingsNavHeaderText">설정</span>
            </div>
            <div id="settingsNavItemsContainer">
                <div id="settingsNavItemsContent">
                    <button class="settingsNavItem" rSc="settingsTabAccount" id="settingsNavAccount" selected>계정</button>
                    <button class="settingsNavItem" rSc="settingsTabMinecraft">마인크래프트</button>
                    <button class="settingsNavItem" rSc="settingsTabMods">모드</button>
                    <button class="settingsNavItem" rSc="settingsTabJava">자바</button>
                    <button class="settingsNavItem" rSc="settingsTabLauncher">런처</button>
                    <div class="settingsNavSpacer"></div>
                    <button class="settingsNavItem" rSc="settingsTabAbout">정보</button>
                    <button class="settingsNavItem" rSc="settingsTabUpdate" id="settingsNavUpdate">업데이트</button>
                    <div id="settingsNavContentBottom">
                        <div class="settingsNavDivider"></div>
                        <button id="settingsNavDone">완료</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="settingsContainerRight">
        <div id="settingsTabAccount" class="settingsTab">
            <div class="settingsTabHeader">
                <span class="settingsTabHeaderText">계정 설정</span>
                <span class="settingsTabHeaderDesc">계정을 추가하거나 삭제합니다.</span>
            </div>
            <div class="settingsAuthAccountTypeContainer">
                <div class="settingsAuthAccountTypeHeader">
                    <div class="settingsAuthAccountTypeHeaderLeft">
                        <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" viewBox="0 0 23 23">
                            <path fill="#f35325" d="M1 1h10v10H1z" />
                            <path fill="#81bc06" d="M12 1h10v10H12z" />
                            <path fill="#05a6f0" d="M1 12h10v10H1z" />
                            <path fill="#ffba08" d="M12 12h10v10H12z" />
                        </svg>
                        <span>Microsoft</span>
                    </div>
                    <div class="settingsAuthAccountTypeHeaderRight">
                        <button class="settingsAddAuthAccount" id="settingsAddMicrosoftAccount">+ Microsoft 계정 추가하기</button>
                    </div>
                </div>
                
                <div class="settingsCurrentAccounts" id="settingsCurrentMicrosoftAccounts">
                    <!-- Microsoft auth accounts populated here. -->
                </div>
            </div>

            <div class="settingsAuthAccountTypeContainer">
                <div class="settingsAuthAccountTypeHeader">
                    <div class="settingsAuthAccountTypeHeaderLeft">
                        <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" viewBox="0 0 9.677 9.667">
